        <RuntimeIdentifier>linux-x64</RuntimeIdentifier>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <Reference Include="TextFile">
        <HintPath>..\..\TextFile.dll</HintPath>
      </Reference>
    </ItemGroup>

    <ItemGroup>
      <None Update="input.txt">
